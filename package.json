{
  "name": "alexa-starter",
  "version": "1.0.0",
  "description": "a simple application for building apps for amazon alexa",
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "prepublish": "npm run build",
    "publish": "serverless deploy",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stephn-R/alexa-starter.git"
  },
  "author": "Stephen Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stephn-R/alexa-starter/issues"
  },
  "homepage": "https://github.com/Stephn-R/alexa-starter#readme",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "@types/alexa-sdk": "^1.0.4",
    "@types/node": "^7.0.12",
    "alexa-sdk": "^1.0.7",
    "ava": "^0.18.2",
    "babel-register": "^6.24.0",
    "serverless": "^1.10.1",
    "typescript": "^2.2.2"
  }
}
